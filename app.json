{
  "expo": {
    "name": "SRIV",
    "slug": "SRIV",
    "version": "1.0.0",
    "scheme": "sriv-scheme",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.joo28.SRIV"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1d58497a-297c-4ecb-892b-86e0b0c33028"
      }
    }
  }
}
